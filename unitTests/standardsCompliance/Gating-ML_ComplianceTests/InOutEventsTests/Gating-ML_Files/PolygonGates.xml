<?xml version="1.0" encoding="UTF-8"?>

<gating:Gating-ML 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:gating="http://www.isac-net.org/std/Gating-ML/v1.1/"
    xsi:schemaLocation="http://www.isac-net.org/std/Gating-ML/v1.1/ ../schema/Gating-ML.v1.1.xsd" >
  
    <!-- Test with a single event in the int-gating_test_file.fcs FCS file 
             * Event: (100, 200) with parameters (FS, SS)
    -->
    
    <!-- First we'll use simple polygon gates which are rectangles -->
    
    <!-- Inside -->
    <gating:PolygonGate gating:id="RectangleInside">
        <gating:dimension gating:parameter="FS" />
        <gating:dimension gating:parameter="SS" />
        <gating:vertex>
        	<gating:coordinate gating:value="50" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="50" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
    </gating:PolygonGate>  
    
    <!-- Inside -->
    <gating:PolygonGate gating:id="RectangleBoundary">
        <gating:dimension gating:parameter="FS" />
        <gating:dimension gating:parameter="SS" />
        <gating:vertex>
        	<gating:coordinate gating:value="100" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="100" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
    </gating:PolygonGate>
    
    <!-- Outside -->
    <gating:PolygonGate gating:id="RectangleOutside">
        <gating:dimension gating:parameter="FS" />
        <gating:dimension gating:parameter="SS" />
        <gating:vertex>
        	<gating:coordinate gating:value="50" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="225" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="50" />
        	<gating:coordinate gating:value="225" />
        </gating:vertex>
    </gating:PolygonGate>
    
    <!-- Tests with concave polygons that look like
       ______
       |    /
       |   /
       |   \
       |____\
       
     -->
    
    <!-- The first three "simple" ones are in the regular non-concave portion
         of the polygon (the left side)
     -->
    
    <!-- Inside -->
    <gating:PolygonGate gating:id="SimpleConcaveInside">
        <gating:dimension gating:parameter="FS" />
        <gating:dimension gating:parameter="SS" />
        <gating:vertex>
        	<gating:coordinate gating:value="50" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="200" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="200" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="200" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="50" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
    </gating:PolygonGate>
    
    <!-- Inside -->
