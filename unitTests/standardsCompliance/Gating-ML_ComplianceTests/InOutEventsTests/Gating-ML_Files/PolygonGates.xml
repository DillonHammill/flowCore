        </gating:vertex>
    </gating:PolygonGate>
    
    <!-- Inside -->
    <gating:PolygonGate gating:id="NonSimpleBoundaryForwardSlant">
        <gating:dimension gating:parameter="FS" />
        <gating:dimension gating:parameter="SS" />
        <gating:vertex>
