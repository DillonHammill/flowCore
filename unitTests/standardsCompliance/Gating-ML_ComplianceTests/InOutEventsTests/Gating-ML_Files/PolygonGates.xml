        	<gating:coordinate gating:value="150" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="50" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
    </gating:PolygonGate>
    
    <!-- Test with a non-simple polygon that looks like an angular hourglass.
          ________
          \      /
           \    /
            \  /
             \/
