        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="250" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="75" />
        	<gating:coordinate gating:value="175" />
        </gating:vertex>
        <gating:vertex>
        	<gating:coordinate gating:value="150" />
        	<gating:coordinate gating:value="150" />
        </gating:vertex>
        <gating:vertex>
